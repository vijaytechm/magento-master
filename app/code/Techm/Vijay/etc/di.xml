<?xml version="1.0"?>
<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
	<!--<preference for="Magento\Catalog\Block\Product\ListProduct" type="Techm\Vijay\Block\Rewrite\Product\ListProduct" />-->
	<type name="Magento\Checkout\Model\Cart">
		<plugin name="TechmCart" type="Techm\Vijay\Plugin\Model\Cart" sortOrder="1" />
	</type>
	<type name="Magento\Catalog\Model\Product">
		<plugin name="TechmProduct" type="Techm\Vijay\Plugin\Model\Product" sortOrder="1" />
	</type>

	<virtualType name="FeedbackGridDataProvider" type="Magento\Framework\View\Element\UiComponent\DataProvider\DataProvider">
		<arguments>
			<argument name="collection" xsi:type="object" shared="false">Techm\Vijay\Model\ResourceModel\Feedback\Collection</argument>
			<argument name="filterPool" xsi:type="object" shared="false">FeedbackGirdFilterPool</argument>
		</arguments>
	</virtualType>

		<virtualType name="FeedbackGirdFilterPool" type="Magento\Framework\View\Element\UiComponent\DataProvider\FilterPool">
		<arguments>
			<argument name="appliers" xsi:type="array">
				<item name="regular" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\RegularFilter</item>
				<item name="fulltext" xsi:type="object">Magento\Framework\View\Element\UiComponent\DataProvider\FulltextFilter</item>
			</argument>
		</arguments>
	</virtualType>

	<type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
		<arguments>
			<argument name="collections" xsi:type="array">
				<item name="feedback_index_listing_data_source" xsi:type="string">Techm\Vijay\Model\ResourceModel\Feedback\Collection</item>
			</argument>
		</arguments>
	</type>

	<virtualType name="Techm\Vijay\Model\ResourceModel\Feedback\Collection" type="Magento\Framework\View\Element\UiComponent\DataProvider\SearchResult">
		<arguments>
			<argument name="mainTable" xsi:type="string">feedback_form</argument>
			<argument name="resourceModel" xsi:type="string">Techm\Vijay\Model\ResourceModel\Feedback</argument>
		</arguments>
	</virtualType>



	<type name="Magento\Framework\View\Element\UiComponent\DataProvider\CollectionFactory">
        <arguments>
            <argument name="collections" xsi:type="array">
                <item name="techm_vijay_post_listing_data_source" xsi:type="string">Techm\Vijay\Model\ResourceModel\Feedback\Collection</item>
            </argument>
        </arguments>
    </type>
    <virtualType name="Techm\Vijay\Model\ResourceModel\Feedback\Collection" type="Magento\Framework\View\Element\UiComponent\DataProvider\SearchResult">
        <arguments>
            <argument name="mainTable" xsi:type="string">feedback_form</argument>
            <argument name="resourceModel" xsi:type="string">Techm\Vijay\Model\ResourceModel\Feedback</argument>
        </arguments>
    </virtualType>


</config>